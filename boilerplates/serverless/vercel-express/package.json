{
	"name": "vercel-express",
	"version": "0.0.1",
  "description": "âš¡ a basic express server boilerplate to run on vercel.",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "homepage": "https://github.com/username/repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "bugs": {
    "url": "https://github.com/username/repository/issues"
  },
  "author": {
    "name": "your name",
    "email": "youremail@gmail.com"
  },
  "types": "./dist/src/index.d.ts",
  "main": "./dist/src/index.js",
	"scripts": {
    "postinstall": "husky install",
    "commit": "cz",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --verbose --watchAll",
    "test:staged": "jest --passWithNoTests --findRelatedTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "start": "node ./dist/src/index.js",
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "dev:install": "npm install -g .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged --concurrent false --quiet",
    "scripts:commits": "ts-node ./resources/update-commits-configs.ts",
    "scripts:issues": "ts-node ./resources/show-all-open-issues.ts"
	},
	"devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@jest/globals": "^29.3.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "commitizen": "^4.2.6",
    "commitizen-emoji": "^1.0.5",
    "commitlint": "^17.4.1",
    "commitlint-config-gitmoji": "^2.2.9",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "rimraf": "^4.0.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
	},
	"dependencies": {
		"express": "^4.18.2"
	}
}
