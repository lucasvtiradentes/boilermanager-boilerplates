{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "declaration": true,
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "ignore",
    "node_modules",
  ]
}
